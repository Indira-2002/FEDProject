function dinning()
{
  var  Visitor_name = document.Dine Booking.Visitor_name;
  var Number_ofMembers = document.Dine Booking.Number_ofMembers;
  var Contact_no = document.Dine Booking.Contact_no;
  var Email = document.Dine Booking.Email;
  var Type_ofFood = document.Dine Booking.Type_ofFood;
  var Room_status = document.Dine Booking.Room_status;
  var Venue_preference = document.Dine Booking.Venue_prefernce;
  if(Visitor_name.value.length<=20)
  {
    alert("name is required")
    Visitor_name.focus();
    return false;
  }
  if(Number_ofMembers.value.length>100)
  {
    alert("Enter in between lessthan 100")
    Number_ofMembers.focus();
    return false;
  }
  if(Contact_no.value.length<0)
  {
    alert("Contact number is required")
    Contact_no.focus();
    return false;
  }
  if(Email.value.length<0)
  {
    alert("Email is required")
    Email.focus();
    return false;
  }
 return false

}
</script>
